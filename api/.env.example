# Copy this file to `.env` and update the values to match your database.
# PostgreSQL connection string format:
# postgresql://USER:PASSWORD@HOST:PORT/DATABASE?sslmode=disable

DATABASE_URL=postgresql://postgres:postgres@localhost:5432/daytona?sslmode=disable

# Daytona SDK
DAYTONA_API_KEY=your_daytona_api_key

# Anthropic Claude Code
ANTHROPIC_API_KEY=your_anthropic_api_key

# Google Gemini
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key

# Browser Use SDK
BROWSER_USE_API_KEY=your_browser_use_api_key

# Inngest Event Key
INNGEST_EVENT_KEY=your_inngest_event_key

# Public URL for the API (used by Claude Code agents running in Daytona sandboxes)
CODE_AGENT_CALLBACK_URL=https://your-public-api-url

# Galileo AI Observability (optional - for tracking and evaluating AI operations)
# Get from: https://app.galileo.ai (Account Settings -> API Keys)
GALILEO_API_KEY=your_galileo_api_key
# Optional: For custom Galileo deployments only
# GALILEO_CONSOLE_URL=https://your-custom-galileo-url
GALILEO_PROJECT=daytona-ux-experiments
GALILEO_LOG_STREAM=production
